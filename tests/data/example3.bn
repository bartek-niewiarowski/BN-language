Def listOperations() {
    numbers = [1, 2, 3, 4, 5, 6]
    numbers.append(7);
    filtredNumbers = numbers.Where(num => num/2 == 0);
    foreach (num in filtredNumbers) {
        print(num);
    }
    foreach (num in numbers.where(n => n/2 == 0)) {
        print(num);
    }
    numbers.Foreach(num =>
    {
        if(num/2 == 0) {
            print(num);} 
    });
    numbers.sort();
}